{
  "name": "typescript-nextjs-starter",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <hey@joaopedro.dev> (@jpedroschmitz)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "clean": "rm -rf .next",
    "dev": "yarn clean && next",
    "build": "NODE_ENV=production next build",
    "build:start": "NODE_ENV=production yarn build && next start",
    "start": "NODE_ENV=production next start",
    "type-check": "tsc",
    "ngrok": "ngrok http 3000",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@udecode/slate-plugins": "0.75.2",
    "axios": "0.27.2",
    "axios-retry": "3.3.1",
    "change-case": "4.1.2",
    "classnames": "2.3.1",
    "cookie": "0.5.0",
    "escape-html": "1.0.3",
    "formik": "2.2.9",
    "headers-polyfill": "3.0.10",
    "js-cookie": "3.0.1",
    "moment": "2.29.4",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-burger-menu": "3.0.8",
    "react-cookie": "4.1.1",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.2",
    "react-headroom": "3.2.0",
    "react-hot-toast": "2.3.0",
    "react-icons": "4.4.0",
    "react-infinite-calendar": "2.3.1",
    "react-loader-spinner": "5.1.7-beta.1",
    "react-paginate": "8.1.3",
    "react-select": "5.4.0",
    "react-use": "17.4.0",
    "sharp": "0.31.0",
    "slate": "0.82.1",
    "slate-history": "0.66.0",
    "slate-hyperscript": "0.77.0",
    "slate-react": "0.82.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.7",
    "@types/change-case": "2.3.1",
    "@types/classnames": "2.3.1",
    "@types/cookie": "0.5.1",
    "@types/escape-html": "1.0.2",
    "@types/js-cookie": "3.0.2",
    "@types/moment": "2.13.0",
    "@types/node": "16.11.41",
    "@types/react": "18.0.14",
    "@types/react-burger-menu": "2.8.3",
    "@types/react-dom": "18.0.5",
    "@types/react-dropzone": "5.1.0",
    "@types/react-headroom": "2.2.2",
    "@types/react-icons": "3.0.0",
    "@types/react-infinite-calendar": "2.3.6",
    "@types/react-paginate": "7.1.1",
    "@types/react-select": "5.0.1",
    "@types/slate": "0.47.11",
    "@types/slate-react": "0.50.1",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "autoprefixer": "10.4.7",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.2",
    "ngrok": "4.3.3",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "tailwindcss": "3.1.4",
    "typescript": "4.7.4",
    "typescript-plugin-css-modules": "3.4.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
